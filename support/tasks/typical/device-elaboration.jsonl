# Osquery
{ "osqueryUpTime":  "osqueryi \"SELECT days, hours, minutes, seconds FROM uptime\" --json", "nature": "json" }
{ "osqueryAllUsers": "osqueryi \"SELECT * from users\" --json", "nature": "json" }
